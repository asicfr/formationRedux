{
  "name": "test2",
  "version": "0.0.1",
  "scripts": {
    "clean": "rm -rf dist",
    "cleanwin": "if exist dist rmdir /s /q dist",
    "build:webpack": "NODE_ENV=production babel-node bin/build",
    "buildwin:webpack": "set NODE_ENV=production&& babel-node bin/build",
    "build": "yarn run clean && yarn run build:webpack",
    "buildwin": "yarn run cleanwin && yarn run buildwin:webpack",
    "prod": "NODE_ENV=production node bin/pm2.js",
    "prodwin": "set NODE_ENV=production&& yarn run start",
    "preview": "yarn run build && yarn run prod",
    "previewin": "yarn run buildwin && yarn run prodwin",
    "start": "node bin/start",
    "lint": "eslint src webpack bin",
    "test": "jest",
    "testwatch": "jest --watch"
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-0"
    ],
    "env": {
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs"
        ]
      }
    },
    "plugins": [
      "transform-decorators-legacy",
      [
        "module-resolver",
        {
          "root": [
            "src"
          ]
        }
      ]
    ]
  },
  "eslintConfig": {
    "extends": [
      "zavatta",
      "zavatta-react"
    ]
  },
  "dependencies": {
    "bootstrap-sass": "3.3.7",
    "react-bootstrap": "0.30.8",
    "react-router-bootstrap": "0.23.1",

    "compression": "1.6.2",
    "es6-shim": "0.35.3",
    "express": "4.15.2",
    "file-loader": "0.9.0",
    "file-stream-rotator": "0.1.0",
    "immutable": "3.8.1",
    "jwt-decode": "2.2.0",
    "lodash": "4.17.4",
    "lodash-fp": "0.10.4",
    "morgan": "1.8.1",
    "radium": "0.18.2",

    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-redux": "5.0.3",
    "react-router": "2.7.0",
    "react-router-redux": "4.0.8",
    
    "recompose": "0.22.0",
    "redial": "0.5.0",
    "redux": "3.6.0",
    "redux-actions": "2.0.1",
    "redux-thunk": "2.2.0",
    "reselect": "3.0.0",
    "serialize-javascript": "1.3.0",
    "superagent": "3.5.2"
  },
  "devDependencies": {
    "app-module-path": "2.2.0",
    "autoprefixer": "6.7.7",
    "autoprefixer-loader": "3.2.0",
    "babel-cli": "6.23.0",
    "babel-core": "6.23.1",
    "babel-eslint": "7.1.1",
    "babel-jest": "19.0.0",
    "babel-loader": "6.3.2",
    "babel-plugin-module-resolver": "2.5.0",
    "babel-plugin-transform-decorators-legacy": "1.3.4",
    "babel-plugin-transform-es2015-modules-commonjs": "6.24.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-es2015": "6.24.0",
    "babel-preset-react": "6.23.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-0": "6.22.0",
    "babel-register": "6.23.0",
    "chai": "3.5.0",
    "css-loader": "0.27.3",
    "enzyme": "2.8.0",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-config-zavatta": "4.2.0",
    "eslint-config-zavatta-react": "2.1.1",
    "eslint-loader": "1.7.0",
    "eslint-plugin-babel": "4.1.1",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.10.3",
    "eslint-plugin-import": "2.2.0",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.10.1",
    "html-webpack-plugin": "2.28.0",
    "ignore-styles": "5.0.1",
    "jest": "19.0.2",
    "node-sass": "4.5.0",
    "postcss-import": "9.1.0",
    "progress-bar-webpack-plugin": "1.9.3",
    "react-addons-test-utils": "15.4.2",
    "redux-mock-store": "1.2.2",
    "sass-loader": "6.0.3",
    "style-loader": "0.16.0",
    "url-loader": "0.5.8",
    "webpack": "2.3.1",
    "webpack-dev-middleware": "1.10.1",
    "webpack-dev-server": "2.4.2",
    "webpack-hot-middleware": "2.17.1",
    "webpack-stats-plugin": "0.1.4"
  },
  "private": true
}
